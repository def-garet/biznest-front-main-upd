{
  "expo": {
    "name": "BIZNest",
    "slug": "BIZNest",
    "scheme": ["biznest"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "assets/imgs/biznest-new.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "assets/imgs/biznest-new.png",
      "resizeMode": "contain",
      "backgroundColor": "#172d55"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "assets/imgs/biznest-new.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.hotaka098.BIZNest",
      "permissions": ["INTERNET"]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "23e9728c-ef83-417f-9f76-381cacdaa835"
      }
    }
  }
}
